FROM kiteco/python-import-exploration

RUN apt update && apt install --no-install-recommends -y libcurl4-openssl-dev libssl-dev
RUN rm -rf /var/lib/apt/lists/*

RUN pip3 install arvados-python-client==1.3.3.20190523182354

ENTRYPOINT ["/usr/bin/env", "python3", "-m", "kite.pkgexploration", "arvados-python-client", "1.3.3.20190523182354", "arvados-python-client__1.3.3.20190523182354.json", "arvados-python-client__1.3.3.20190523182354.log"]
