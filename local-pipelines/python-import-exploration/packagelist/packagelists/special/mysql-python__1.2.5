FROM kiteco/python-import-exploration

RUN apt update && apt install --no-install-recommends -y libmysqlclient-dev
RUN rm -rf /var/lib/apt/lists/*

RUN pip2 install mysql-python==1.2.5

ENTRYPOINT ["/usr/bin/env", "python2", "-m", "kite.pkgexploration", "MySQL-python", "1.2.5", "mysql-python__1.2.5.json", "mysql-python__1.2.5.log"]
