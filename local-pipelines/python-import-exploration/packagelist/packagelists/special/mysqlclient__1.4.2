FROM kiteco/python-import-exploration

RUN apt update && apt install --no-install-recommends -y libmysqlclient-dev
RUN rm -rf /var/lib/apt/lists/*

RUN pip2 install mysqlclient==1.4.2

ENTRYPOINT ["/usr/bin/env", "python2", "-m", "kite.pkgexploration", "mysqlclient", "1.4.2", "mysqlclient__1.4.2.json", "mysqlclient__1.4.2.log"]
